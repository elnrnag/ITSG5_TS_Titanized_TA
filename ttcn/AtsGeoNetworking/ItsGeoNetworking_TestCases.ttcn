            */
            testcase TC_GEONW_CAP_FPB_BV_02() runs on ItsMtc system ItsGeoNetworkingSystem {
                
                f_GEONW_CAP_FPB_BV_02();
                    
            } // end TC_GEONW_CAP_FPB_BV_02
                
        } // end geoCapForwardingPacketBuffer
        
    } // end geoCapacities
    
