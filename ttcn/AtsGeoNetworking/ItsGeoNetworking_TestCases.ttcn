            testcase TC_GEONW_PON_GUC_BV_08() runs on ItsMtc system ItsGeoNetworkingSystem {
                
                f_GEONW_PON_GUC_BV_08();
                    
            } // end TC_GEONW_PON_GUC_BV_08
                
