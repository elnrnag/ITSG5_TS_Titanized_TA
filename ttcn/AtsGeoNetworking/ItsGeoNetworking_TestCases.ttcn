            */
            testcase TC_GEONW_PON_GUC_BO_13() runs on ItsMtc system ItsGeoNetworkingSystem {
                
                f_GEONW_PON_GUC_BO_13();
                    
            } // end TC_GEONW_PON_GUC_BV_13
                
        } // end geoGeoUnicast
        
        // 6.2.2.9
        group geoGeoBroadcast {
            
            /**
